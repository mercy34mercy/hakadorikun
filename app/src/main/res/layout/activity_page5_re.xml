<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".page5_re">


    <TextView
        android:id="@+id/toolbar_page5_re"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_page5_re">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_weight="0"
            android:text="課題"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="80dp" />

        <ListView
            android:id="@+id/List_Task"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:layout_constraintTop_toBottomOf="@+id/textView31"
            tools:layout_editor_absoluteX="-16dp" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_weight="0"
            android:text="予定"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/add_task" />

        <ListView
            android:id="@+id/List_Event"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textFilterEnabled="false"
            app:layout_constraintTop_toBottomOf="@+id/textView33"
            tools:layout_editor_absoluteX="-16dp">

        </ListView>

    </LinearLayout>

    <androidx.constraintlayout.motion.widget.MotionLayout
        android:id="@+id/motionLayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layoutDescription="@xml/scene_09"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:showPaths="true">

        <View
            android:id="@+id/view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp" />

        <TextView
            android:id="@+id/sub_text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="課題"
            tools:layout_editor_absoluteX="200dp"
            tools:layout_editor_absoluteY="553dp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/orange"
            android:src="@drawable/ic_plus"
            app:fabCustomSize="48dp"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="355dp" />

        <TextView
            android:id="@+id/sub_text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="予定"
            tools:layout_editor_absoluteX="297dp"
            tools:layout_editor_absoluteY="714dp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/sub_btn1_page5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/orange"
            android:clickable="true"
            android:src="@drawable/ic_baseline_menu_book_24"
            app:fabCustomSize="32dp"
            app:layout_constraintBottom_toTopOf="@+id/sub_btn2_page5"
            app:layout_constraintTop_toBottomOf="@+id/fab"
            tools:layout_editor_absoluteX="332dp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/sub_btn2_page5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/orange"
            android:clickable="true"
            android:src="@drawable/ic_baseline_calendar_today_24"
            app:fabCustomSize="32dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view"
            tools:layout_editor_absoluteX="363dp" />


    </androidx.constraintlayout.motion.widget.MotionLayout>


</androidx.constraintlayout.widget.ConstraintLayout>